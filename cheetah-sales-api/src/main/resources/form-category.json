{
	"name": "Category-form",
	"fields": [
		
		{
			"fieldGroupClassName": "row",
			"fieldGroup": [
				{
					"className": "col-md-3",
					"type": "input",
					"key": "name",
					"templateOptions": {
						"label": "Category Name",
						"attributes": {
							"class": "form-control form-control-sm"
						}
					}
				},{
					"className": "col-md-3",
					"type": "picklist",
					"key": "parent",
					"templateOptions": {
						"label": "Category",
						"val": "id",
						"domain": "CATEGORY",
						"attributes": {
							"class": "form-control form-control-sm"
						}
					}
				},
				
				{
					"className": "col-md-3",
					"type": "input",
					"key": "order",
					"templateOptions": {
						"label": "Order",
						"required": "true",
						"attributes": {
							"class": "form-control form-control-sm"
						}
					}
				}
					
			]
		},
		{
			"fieldGroupClassName": "row",
			"fieldGroup": [
				{
					"className": "col-md-12",
					"type": "textarea",
					"key": "description",
					"templateOptions": {
						"label": "Description",
						"rows": "2",
						"attributes": {
							"class": "form-control form-control-sm"
						}
					}
				}
			]
		}
	]
}